# noexec на флешке #!/bin/bash

pdflatex labs
# bibtex labs
pdflatex labs
# pdflatex labs

# utf-8 не годится, нужна однобайтовая кодировка (любая подходит, но официально результат rumakeindex в koi8)
LANG=ru_RU.KOI8-RU rumakeindex labs.idx
iconv -f KOI8-RU -t WINDOWS-1251 labs.ind -o labs.ind
pdflatex labs 


